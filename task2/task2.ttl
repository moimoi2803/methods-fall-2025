@prefix : <http://www.semanticweb.org/daucatmoi/ontologies/2025/9/family-task2/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

### ОНТОЛОГИЯ ###
<http://www.semanticweb.org/daucatmoi/ontologies/2025/9/family-task2> rdf:type owl:Ontology ;
    rdfs:label "Family Ontology - Task 2 (daucatmoi)" .

### КЛАСС ###
:Человек rdf:type owl:Class ;
    rdfs:label "Person" .

### СВОЙСТВА ###
:ИмеетРодителя rdf:type owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:label "hasParent" ;
    rdfs:domain :Человек ;
    rdfs:range :Человек ;
    owl:propertyDisjointWith :ИмеетРодителя .

:ИмеетПредка rdf:type owl:ObjectProperty ;
    rdfs:label "hasAncestor" ;
    owl:propertyChainAxiom ( :ИмеетРодителя :ИмеетПредка ) .

:ИмеетПотомка rdf:type owl:ObjectProperty ;
    rdfs:label "hasDescendant" ;
    owl:propertyChainAxiom ( :ИмеетПотомка :ИмеетРодителя ) .

:ЯвляетсяРодителемДля rdf:type owl:ObjectProperty ;
    rdfs:label "isParentOf" ;
    owl:inverseOf :ИмеетРодителя .

### СУЩНОСТИ ###
:GGParent rdf:type :Человек ;
    rdfs:label "GGParent" .

:GParent rdf:type :Человек ;
    rdfs:label "GParent" ;
    :ИмеетРодителя :GGParent .

:GParent2 rdf:type :Человек ;
    rdfs:label "GParent2" ;
    :ИмеетРодителя :GGParent .

:Parent rdf:type :Человек ;
    rdfs:label "Parent" ;
    :ИмеетРодителя :GParent .

:Parent2 rdf:type :Человек ;
    rdfs:label "Parent2" ;
    :ИмеетРодителя :GParent2 .

:Child rdf:type :Человек ;
    rdfs:label "Child" ;
    :ИмеетРодителя :Parent .

:Child2 rdf:type :Человек ;
    rdfs:label "Child2" ;
    :ИмеетРодителя :Parent2 .

:Child3 rdf:type :Человек ;
    rdfs:label "Child3" ;
    :ИмеетРодителя :Parent2 .
